{{#partial "content"}}
    <div class="row">
        <div class="col-md-6">
            <h3>Supply: {{supply.farmerName}} - {{supply.productName}}</h3>
            <p>Farmer: <a href="/farmers/{{supply.farmerId}}">{{supply.farmerName}}</a></p>
            <p>Product: <a href="/products/{{supply.productId}}">{{supply.productName}}</a></p>
            <p>Quantity: {{supply.quantity}} </p>
        </div>
    </div>
{{/partial}}
{{> layout.hbs}}