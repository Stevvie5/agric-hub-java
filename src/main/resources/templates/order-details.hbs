{{#partial "content"}}
    <div class="row">
        <div class="col-md-6">
            <h3>Order: {{order.customerName}} - {{order.productName}}</h3>
            <p>Customer: <a href="/customers/{{order.customerId}}">{{order.customerName}}</a></p>
            <p>Product: <a href="/products/{{order.productId}}">{{order.productName}}</a></p>
            <p>Quantity: {{order.quantity}} </p>
        </div>
    </div>


{{/partial}}
{{> layout.hbs}}